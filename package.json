    "startweb": "cd ./apps/web/ && pnpm run start"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "dotenv-cli": "latest",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18"
  }
}
